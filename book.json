{
  "root": "./site",
  "title": "Scrolliris Help",
  "author": "Lupine Software LLC",
  "language": "en",
  "structure": {
    "readme": "README.adoc",
    "summary": "TOC.adoc"
  },
  "plugins": [
    "theme-faq",
    "favicon",
    "-fontsettings",
    "-sharing"
  ],
  "pluginsConfig": {
    "favicon": {
      "shortcut": "_assets/favicon.ico",
      "bookmark": "_assets/favicon.ico",
      "appleTouch": "_assets/img/touch-icon-180.png",
      "appleTouchMore": {
        "120x120": "_assets/img/touch-icon-120.png",
        "180x180": "_assets/img/touch-icon-180.png"
      }
    }
  }
}
